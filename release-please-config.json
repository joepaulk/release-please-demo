{
  "$schema": "https://github.com/googleapis/release-please/raw/main/schemas/config.json",
  "release-type": "node",
  "always-link-local": true,
  "plugins": [
    {
      "type": "node-workspace",
      "merge": false
    }
  ],
  "packages": {
    ".": {
      "release-type": "node",
      "versioning": "prerelease",
      "prerelease-type": "alpha",
      "prerelease": true
    }
  }
}
